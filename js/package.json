{
  "name": "coderetreat-js",
  "version": "1.0.0",
  "description": "Template for ES6 js testing",
  "main": "src/main.js",
  "keywords": [
    "coderetreat"
  ],
  "author": "Tomas Latal",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill",
    "test:watch": "npm run test -- -w",
    "clean" : "git reset HEAD --hard && git clean -f -d"
  },
  "devDependencies": {
    "mocha": "^2.5.3"
  }
}
